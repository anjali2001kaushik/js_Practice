22. Write a function to find the second largest number in an array.
		ex:- var arr = [12,4,6,9,2,15,3,9];
		output second largest : 12


function seclargest(){
    var arr=[12,4,6,9,2,15,3,9];
    var secmax=0;
    var max=0;
    for(var i=0;i<arr.length;i++){
        if(arr[i]>max){
            secmax=max;
            max=arr[i];
        }
        else if(arr[i]>secmax && arr[i]!=max){
            secmax=arr[i];
        }
    }
    return secmax;
}
undefined
seclargest();
12



